{
    "extends": [
        "@rocketseat/eslint-config/node"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "camelcase": "off",
      "no-useless-constructor": "off",
      "@typescript-eslint/no-explicit-any": "off",
      "prettier/prettier": [
        "error",
        {
          "endOfLine": "auto"
        }
      ]
    }
}